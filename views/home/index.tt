[% PROCESS util/format.tt %]
<div align="center">
    <table width="100%">
        <tr>
            <td valign="top" colspan="2">
                <div class="header">
                    <a href="http://[% session.user.jira_host %]">Tickets</a>
                </div>
                <div>[% PROCESS home/tickets.tt %]</div>
            </td>
        </tr>
        <tr>
            <td class="default" valign="top" width="62%">
                <table width="100%">
                    <tr>
                        <td valign="top">
                            <div class="header">
                                This Week's Entries
                                (<a href="/reports/by_category">Category</a>
                                 |
                                 <a href="/reports/by_day">Daily</a>
                                 |
                                 <a href="/reports/summary">Summary</a>
                                )
                            </div>
                            <div>[% PROCESS home/entries.tt %]</div>
                        </td>
                    </tr>
                </table>
            </td>
            <td valign="top">
                <table width="100%">
                    <tr>
                        <td>[% under_construction("HOME/CURRENT_RELEASE"); %]</td>
                    </tr>
                    <tr>
                        <td>
                            <div class="header">
                                <a href="/todos">Todos</a>
                            </div>
                            <div>[% PROCESS home/todos.tt %]</div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="header">
                                <a href="/goals">Goals</a>
                            </div>
                            <div>[% PROCESS home/goals.tt %]</div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="header">Recent Notes</div>
                            <div>[% PROCESS home/notes.tt notes=recent_notes %]</div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="header">Favorite Notes</div>
                            <div>[% PROCESS home/notes.tt notes=favorite_notes %]</div>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <div>[% PROCESS home/countdowns.tt %]</div>
            </td>
        </tr>
    </table>
</div>
